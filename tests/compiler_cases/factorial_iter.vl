void main(void)
{
	int a, res;
	print("Enter an integer to compute its factorial:");
	a = readInt();
	if (a < 0) {
		print("\nFactorial of a negative integer is undefined!\n");
	}
	else {
		res = 1;
		while (a > 0) {
			res = res * a; a = a - 1;
		}
		print("\nThe result is: ", res, "\n");
	}
}